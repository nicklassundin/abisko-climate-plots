{
  "sourceType": "module",
  "source": {
    "include": ["server.js"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/better-docs",
    "encoding": "utf8",
    "destination": "docs/"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}