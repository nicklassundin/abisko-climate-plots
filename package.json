  "scripts": {
    "prepublish": "npm-auto-version",
    "publish": "npm publish --access public",
    "postpublish": "git push origin --tags",
    "update": "npm update climate-plots-config && npm update climate-plots-glimworks-reader",
    "test": "git submodule update && npm run test-unit",
    "test-respons": "artillery quick --count 10 -n 20 https://localhost/charts",
    "test-unit": "mocha './modules/**/*.spec.js' --delay",
    "test-dev": "mocha  './modules/**/*.spec.js' '*.spec.dev.js' 'modules/**/*.spec.dev.js' --delay",
    "start-no-update": "node server.js",
    "start": "npm run update && git submodule update && node server.js",
    "start-dev": "npm run update && npx nodemon -x \"clear;node server.js d\"",
    "start-inspect": "npm run update && npx nodemon -x \"clear;node --inspect server.js d\"",
    "start-full": "npm run update && git submodule update && npm test && npx nodemon server.js",
    "static": "cp client/bundle.js static/bundle.js",
    "lint": "eslint *.js ./modules/",
    "lint-fix": "eslint --fix *.js ./modules/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicklassundin/abisko-climate-plots.git"
  },
  "keywords": [],
  "author": "",
