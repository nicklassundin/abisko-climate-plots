  },
  "homepage": "https://github.com/nicklassundin/abisko-climate-plots#readme",
  "nodemonConfig": {
    "ignore": [
      "client/*.js",
      "docs/*",
      "temp/*",
      "static/*",
      "cms/*",
      "node_modules/*",
      "index.html"
    ],
    "events": {
      "start": "webpack --mode=development"
    }
  }
